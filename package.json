{
  "name": "ogsc",
  "version": "0.1.0",
  "engine": {
    "node": "12.18.3"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "db:migrate": "ts-node -O '{ \"module\": \"commonjs\" }' $(yarn bin db-migrate)",
    "db:migrate:create": "ts-node -O '{ \"module\": \"commonjs\" }' bin/create-migration.ts",
    "db:seed": "ts-node -O '{ \"module\": \"commonjs\" }' bin/seed-database.ts",
    "heroku-postbuild": "yarn db:migrate up"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{css,scss,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^1.0.0",
    "@prisma/client": "2.8.0",
    "autoprefixer": "9.8.6",
    "db-migrate": "^0.11.11",
    "db-migrate-base": "^2.3.0",
    "faker": "^5.1.0",
    "joi": "^17.2.1",
    "little-state-machine": "^3.1.2",
    "next": "9.5.3",
    "next-auth": "^3.1.0",
    "ora": "^5.1.0",
    "postcss-import": "^12.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.9.6",
    "tailwindcss": "^1.9.4"
  },
  "devDependencies": {
    "@prisma/cli": "2.8.0",
    "@types/db-migrate-base": "^0.0.8",
    "@types/faker": "^5.1.3",
    "@types/next-auth": "^3.1.10",
    "@types/node": "^14.11.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-eslint": "^10.1.0",
    "db-migrate-pg": "^1.2.2",
    "db-migrate-plugin-typescript": "ethanlee16/plugin-typescript",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "config": {}
}
